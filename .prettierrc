{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "useTabs": false,
  "importOrder": [
    "^(react$)|(react-native$)",
    "^native-base",
    "^@native-base/(.*)$",
    "^(expo*)",
    "(react-*)|(react-native-*)",
    "<THIRD_PARTY_MODULES>",
    "^@(.*)/?(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
